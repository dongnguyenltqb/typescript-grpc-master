{
  "name": "@dong/master",
  "version": "0.0.12",
  "types": "dist/src/index.d.ts",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "dev": "ts-node-dev --notify --respawn src/index.ts ",
    "build": "npm run lint && rm -Rf dist && npm run build:proto && tsc && cp -R src/protobuf dist/src/",
    "start": "node dist/src/index.js",
    "build:proto:message": "./node_modules/.bin/proto-loader-gen-types --keepCase=true --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js -I $PWD/src/protobuf -I $PWD/src/protobuf/message --outDir=src/proto-gen $(ls src/protobuf/message)",
    "build:proto:rpc": "./node_modules/.bin/proto-loader-gen-types --keepCase=true --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js -I $PWD/src/protobuf -I $PWD/src/protobuf/rpc --outDir=src/proto-gen $(ls src/protobuf/rpc)",
    "build:proto": "rm -R src/proto-gen && npm run build:proto:message && npm run build:proto:rpc",
    "release": "npm run build && npm publish",
    "db:migrate:new": "knex migrate:make -x ts",
    "db:migrate:latest": "knex migrate:latest"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2",
    "ts-node-dev": "^1.1.8"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.5.7",
    "@grpc/proto-loader": "^0.6.9",
    "dotenv": "^16.0.0",
    "google-protobuf": "^3.20.0-rc.1",
    "knex": "^1.0.3",
    "pg": "^8.7.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3"
  }
}
